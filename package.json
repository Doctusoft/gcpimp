{
  "name": "gcpimp",
  "version": "1.0.0",
  "description": "GCP Developer tools - Doctusoft Hackaton 2017",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean:dist": "rimraf dist",
    "clean:cache": "rimraf .cache",
    "clean:packaged": "rimraf packaged",
    "clean": "run-p clean:*",
    "build:cs-toolbar": "parcel build ./src/content-scripts/custom-toolbar-color.ts",
    "build:popup-html": "parcel build ./src/popup.html",
    "build": "run-p build:*",
    "package:keygen": "crx keygen",
    "package:move-key": "cpx key.pem packaged && rimraf key.pem",
    "package:crx": "crx pack dist -o gcpimp.crx -p packaged/key.pem",
    "package:move": "cpx gcpimp.crx packaged && rimraf gcpimp.crx",
    "package": "run-s clean build package:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Doctusoft/gcpimp.git"
  },
  "author": "Doctusoft",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Doctusoft/gcpimp/issues"
  },
  "homepage": "https://github.com/Doctusoft/gcpimp#readme",
  "devDependencies": {
    "@types/chrome": "0.0.69",
    "cpx": "^1.5.0",
    "crx": "^3.2.1",
    "npm-run-all": "^4.1.3",
    "parcel": "^1.9.3",
    "rimraf": "^2.6.2",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {},
  "browserlist": [
    "last 2 Chrome versions"
  ]
}
