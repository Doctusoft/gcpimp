{
  "name": "gcpimp",
  "version": "1.0.0",
  "description": "GCP Developer tools - Doctusoft Hackaton 2017",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean:dist": "rimraf dist",
    "clean:packaged": "rimraf packaged",
    "clean": "run-p clean:*",
    "build:ts": "tsc",
    "build:copy-assets": "cpx src/assets/**/* dist/assets",
    "build:manifest": "cpx src/manifest.json dist",
    "build:html": "cpx src/**/*.html dist",
    "build": "run-p build:*",
    "package:keygen": "crx keygen",
    "package:move-key": "cpx key.pem packaged && rimraf key.pem",
    "package:crx": "crx pack dist -o gcpimp.crx -p packaged/key.pem",
    "package:move": "cpx gcpimp.crx packaged && rimraf gcpimp.crx",
    "package": "run-s clean build package:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Doctusoft/gcpimp.git"
  },
  "author": "Doctusoft",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Doctusoft/gcpimp/issues"
  },
  "homepage": "https://github.com/Doctusoft/gcpimp#readme",
  "devDependencies": {
    "@types/chrome": "0.0.47",
    "crx": "^3.2.1",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2",
    "cpx": "^1.5.0"
  },
  "dependencies": {}
}
