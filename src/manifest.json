{
	"manifest_version": 2,
	"name": "GCP enhancer by DS",
	"description": "This extension adds extra functionality to various GCP console pages",
	"version": "0.1",
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": {
			"85": "assets/gcpimplogo.png",
			"16": "assets/gcpimplogo16.png",
			"48": "assets/gcpimplogo48.png",
			"128": "assets/gcpimplogo128.png"
		},
		"default_title": "GCP enhancer",
		"default_popup": "popup.html"
	},
	"author": "Doctusoft Ltd",
	"permissions": [
		"https://console.cloud.google.com/",
		"https://bigquery.cloud.google.com/",
		"https://www.googleapis.com/",
		"https://cloudpricingcalculator.appspot.com/",
		"https://accounts.google.com/",
		"https://accounts.google.com/o/oauth2/client_log",
		"tabs",
		"activeTab",
		"identity"
	],
	"key": "",
	"oauth2": {
		"client_id": "587753390108-8psejr11d0i4el8bromlt4r82t97gutb.apps.googleusercontent.com",
		"scopes": ["profile"]
	}
}
